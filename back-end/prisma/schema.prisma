generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Customer{
  id                Int               @id @default(autoincrement())
  name              String
  identidade        String
  birth_day         DateTime? // ? = pode ser null
  street_name       String
  house_number      String
  complements       String?
  distric           String
  municipality      String
  state             String
  telefone          String
  email             String
}